doctype html
html(ng-app='ngApp')
  head
    meta(charset='utf-8')
    title DashForce
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/css/materialize.min.css')
    link(href='https://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
    link(rel='stylesheet', href='stylesheets/style.css')
    link(href='https://fonts.googleapis.com/css?family=Open+Sans', rel='stylesheet')
  body#body(ng-controller='RegisterController')
    nav(style='height: 7em!important;')
      .nav-wrapper
        a.brand-logo(href='http://www.wforce.org/')
          img#wos-logo(src='./public/img/WOS Logo_Transparent.png', alt='WOS Logo')
    .body
      #registration.col-md-6.col-md-offset-3
        h2(style='text-align:center; color:white; border-bottom: solid 2px white;\
        padding-bottom: 0.5em; margin-top: 0px;') Register
        form(action='/register' method='post' name='form', ng-submit='vm.register()', role='form')
          .form-group(ng-class="{ 'has-error': form.firstName.$dirty && form.firstName.$error.required }")
            label(style='color:white; font-size: 20px;', for='username') First name:
            input#firstName.form-control(type='text', name='firstName', ng-model='vm.user.firstName', required='')
            // <span ng-show="form.firstName.$dirty && form.firstName.$error.required" class="help-block">First name is required</span>
          .form-group(ng-class="{ 'has-error': form.lastName.$dirty && form.lastName.$error.required }")
            label(style='color:white; font-size: 20px;', for='username') Last name:
            input#Text1.form-control(type='text', name='lastName', ng-model='vm.user.lastName', required='')
            // <span ng-show="form.lastName.$dirty && form.lastName.$error.required" class="help-block">Last name is required</span>
          .form-group(ng-class="{ 'has-error': form.username.$dirty && form.username.$error.required }")
            label(style='color:white; font-size: 20px;', for='email') Email:
            input#email.form-control(type='text', name='email', ng-model='vm.user.username', required='')
            // <span ng-show="form.username.$dirty && form.username.$error.required" class="help-block">Email is required</span>
          .form-group(ng-class="{ 'has-error': form.password.$dirty && form.password.$error.required }")
            label(style='color:white; font-size: 20px;', for='password') Password:
            input#password.form-control(type='password', name='password', ng-model='vm.user.password', required='')
            // <span ng-show="form.password.$dirty && form.password.$error.required" class="help-block">Password is required</span>
          .form-actions
            button.btn.btn-primary(type='submit', ng-disabled='form.$invalid || vm.dataLoading') Register
            // <img ng-if="vm.dataLoading" src="data:image/gif;base64," />
            a.btn.btn-link(href='/') Cancel
          p(style='color:white; text-align: center;')
            i.material-icons(style='font-size:15px; color:#78a7b2') new_releases
            |  = required input
